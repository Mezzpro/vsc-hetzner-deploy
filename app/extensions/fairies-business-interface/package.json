{
  "name": "fairies-business-interface",
  "displayName": "Fairies Business Interface",
  "description": "Business interface with fairies.ai styling - multi-tab workspace experience",
  "version": "1.0.0",
  "publisher": "fairies-systems",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fairies.dashboard",
        "title": "Business Dashboard",
        "category": "Fairies",
        "icon": "$(dashboard)"
      },
      {
        "command": "fairies.analytics", 
        "title": "Analytics Center",
        "category": "Fairies",
        "icon": "$(graph)"
      },
      {
        "command": "fairies.downloads",
        "title": "Download Center", 
        "category": "Fairies",
        "icon": "$(cloud-download)"
      },
      {
        "command": "fairies.tools",
        "title": "Business Tools",
        "category": "Fairies",
        "icon": "$(tools)"
      },
      {
        "command": "fairies.workspace",
        "title": "Create Business Workspace",
        "category": "Fairies",
        "icon": "$(layout)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "fairies-business-nav",
          "name": "Business Navigation",
          "when": "workspaceFolderCount != 0"
        }
      ]
    },
    "keybindings": [
      {
        "command": "fairies.dashboard",
        "key": "ctrl+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "fairies.analytics",
        "key": "ctrl+shift+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}