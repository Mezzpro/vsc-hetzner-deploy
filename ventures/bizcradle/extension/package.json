{
    "name": "bizcradle-downloads-extension",
    "displayName": "Bizcradle Downloads",
    "description": "Simple download center for Bizcradle marketing applications",
    "version": "1.0.0",
    "publisher": "bizcradle",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "bizcradleDownloads",
                    "name": "BizCradle Downloads",
                    "when": "resourceExtname"
                }
            ]
        },
        "commands": [
            {
                "command": "bizcradle.downloads",
                "title": "Download Center",
                "category": "Bizcradle",
                "icon": "$(cloud-download)"
            },
            {
                "command": "bizcradle.downloadItem",
                "title": "Download Item",
                "category": "Bizcradle"
            },
            {
                "command": "bizcradle.toggleSelection",
                "title": "Toggle Selection",
                "category": "Bizcradle"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "16.x",
        "typescript": "^4.9.4"
    }
}