{
  "name": "cradle-downloads-extension",
  "displayName": "CradleSystem Downloads",
  "description": "Simple download center for CradleSystem business applications",
  "version": "1.0.0",
  "publisher": "cradlesystem",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cradle.downloads",
        "title": "Downloads",
        "category": "CradleSystem"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}