{
  "name": "mezzpro-downloads-extension",
  "displayName": "MezzPro Downloads",
  "description": "Simple download center for MezzPro blockchain applications",
  "version": "1.0.0",
  "publisher": "mezzpro",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mezzpro.downloads",
        "title": "Download Center",
        "category": "MezzPro",
        "icon": "$(cloud-download)"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}