{
  "workbench.colorTheme": "Dark High Contrast",
  "window.title": "MezzPro - ${activeEditorShort}",
  "workbench.startupEditor": "none",
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "workbench.tips.enabled": false,
  "workbench.panel.visible": false,
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": false,
  "workbench.view.terminal.visible": false,
  "workbench.view.outline.visible": false,
  "workbench.view.timeline.visible": false,
  "outline.showFiles": false,
  "outline.showModules": false,
  "outline.showNamespaces": false,
  "outline.showPackages": false,
  "outline.showClasses": false,
  "outline.showMethods": false,
  "outline.showProperties": false,
  "outline.showFields": false,
  "outline.showConstructors": false,
  "outline.showEnums": false,
  "outline.showInterfaces": false,
  "outline.showFunctions": false,
  "outline.showVariables": false,
  "outline.showConstants": false,
  "outline.showStrings": false,
  "outline.showNumbers": false,
  "outline.showBooleans": false,
  "outline.showArrays": false,
  "outline.showObjects": false,
  "outline.showKeys": false,
  "outline.showNull": false,
  "timeline.showView": false,
  "workbench.view.explorer.openEditors.visible": false,
  "explorer.openEditors.visible": 0,
  "terminal.integrated.showOnStartup": "never",
  "telemetry.telemetryLevel": "off",
  "update.showReleaseNotes": false,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.vscode": true
  },
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "breadcrumbs.enabled": false,
  "workbench.editor.showTabs": false,
  "workbench.sideBar.location": "left",
  "workbench.colorCustomizations": {
    "activityBar.background": "#0F172A",
    "activityBar.foreground": "#F8FAFC",
    "sideBar.background": "#1E293B",
    "sideBar.foreground": "#F8FAFC",
    "editor.background": "#0F172A",
    "editor.foreground": "#F8FAFC",
    "panel.background": "#1E293B",
    "statusBar.background": "#334155",
    "statusBar.foreground": "#F8FAFC",
    "titleBar.activeBackground": "#1E293B",
    "titleBar.activeForeground": "#F8FAFC",
    "titleBar.inactiveBackground": "#0F172A",
    "titleBar.inactiveForeground": "#94A3B8",
    "titleBar.border": "#334155",
    "tab.activeBackground": "#1E293B",
    "tab.activeForeground": "#F8FAFC",
    "tab.inactiveBackground": "#0F172A",
    "tab.inactiveForeground": "#94A3B8"
  }
}
